{
  "name": "triniti/curator",
  "description": "Php library that provides implementations for triniti:curator schemas.",
  "type": "library",
  "license": "Apache-2.0",
  "require": {
    "php": ">=7.1",
    "gdbots/ncr": "~0.3",
    "symfony/http-foundation": "^4.1",
    "twig/twig": "^2.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.4",
    "symfony/cache": "^4.1",
    "triniti/acme-schemas": "~0.6"
  },
  "autoload": {
    "psr-4": {
      "Triniti\\Curator\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Triniti\\Tests\\Curator\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
