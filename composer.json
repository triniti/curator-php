{
  "name": "triniti/curator",
  "description": "Php library that provides implementations for triniti:curator schemas.",
  "type": "library",
  "license": "Apache-2.0",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/triniti/sys-php"
    },
    {
      "type": "vcs",
      "url": "https://github.com/triniti/schemas"
    }
  ],
  "require": {
    "php": ">=7.4",
    "gdbots/ncr": "^2.0",
    "symfony/http-foundation": "^4.2",
    "triniti/schemas": "dev-sy5",
    "triniti/sys": "dev-sy5",
    "twig/twig": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.1",
    "symfony/cache": "^4.1",
    "triniti/acme-schemas": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Triniti\\Curator\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Triniti\\Tests\\Curator\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
